namespace GS;

table GameState {
    players:[Player];
    cards_on_ground:[PlayingCard];
}

table Player {
    id:int;
    x:int;
    y:int;
    rat_type:int;
    frame:int;
    rotation:int;
    message:string;
}

table PlayingCard {
    value:int;
    x:int;
    y:int;
    owner_id:int;
}

root_type GameState;